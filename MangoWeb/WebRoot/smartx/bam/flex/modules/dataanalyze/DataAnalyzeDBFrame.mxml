<?xml version="1.0" encoding="utf-8"?>
<mx:Module xmlns:fx="http://ns.adobe.com/mxml/2009" 
		   xmlns:s="library://ns.adobe.com/flex/spark" 
		   xmlns:mx="library://ns.adobe.com/flex/mx" layout="absolute" width="100%" height="100%"
		   creationComplete="init()"
		   >
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			
			protected function init():void
			{
				var panel:DataAnalyzeDBPanel = new DataAnalyzeDBPanel();
				if(listVbox.getChildren().length>0){
					listVbox.removeAllChildren();
				}
				listVbox.addChild(panel);
			}
			
		]]>
	</fx:Script>
	<mx:VBox id="listVbox" height="100%" width="100%" >
		
	</mx:VBox>
</mx:Module>
