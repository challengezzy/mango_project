/* CSS file */
@namespace "net.brandonmeyer.containers.*";
@namespace mx "library://ns.adobe.com/flex/mx";
@namespace chartClasses "mx.charts.chartClasses.*";
@namespace calendarDisplayClasses "qs.controls.calendarDisplayClasses.*";
@namespace s "library://ns.adobe.com/flex/spark";
mx|Application,s|Application {
	backgroundColor: #f2f2f2;
	backgroundGradientAlphas: 0, 0;
	themeColor: #949494;
	fontSize: 12;
}
mx|Module{
	fontSize: 12;
}
mx|ToolTip {
	cornerRadius: 0;
	color: #333333;
	drop-shadow-enabled:false;
	dropShadowEnabled: false;
	fontSize: 12; 
}
SuperPanel
{
	resize-grip-skin:          	Embed(source="../images/resizeHandler.png");
	
	actionAlpha:		 		0.75;
    border-color: 				#ffffff;
    border-alpha: 				1;
    border-thickness-left: 		0;
    border-thickness-top: 		0;
    border-thickness-bottom:	9;
    border-thickness-right: 	0;
    corner-radius: 				1;
    highlight-alphas: 			0.1, 0;
    header-colors: 				#666666, #333333;
    footer-colors: 				#ffffff, #ffffff;
    dropShadowEnabled: 			true;
    shadow-distance:			4;
    
    title-style-name:			"mypanelTitle";
    close-button-style-name:	"closeButton";
    minimize-button-style-name:	"minimizeButton";
    maximize-button-style-name:	"maximizeButton";
    
    inactive-header-colors: 	#999999, #666666;
    inactive-footer-colors: 	#ffffff, #ffffff;
	inactive-shadow-distance:	1;
	inactive-title-style-name:	"mypanelInactiveTitle";
}

.closeButton
{
	icon: 						Embed(source="../images/icon_close_up.png");
	up-icon: 					Embed(source="../images/icon_close_up.png");
	over-icon: 					Embed(source="../images/icon_close_over.png");
	down-icon: 					Embed(source="../images/icon_close_down.png");
	disabled-skin: 				Embed(source="../images/clear.png");
	selected-up-skin: 			Embed(source="../images/clear.png");
	selected-over-skin: 		Embed(source="../images/clear.png");
	selected-down-skin: 		Embed(source="../images/clear.png");
    down-skin: 					Embed(source="../images/clear.png");
    over-skin: 					Embed(source="../images/clear.png");
    up-skin: 					Embed(source="../images/clear.png");	
}

.maximizeButton
{
	icon: 						Embed(source="../images/icon_expand_up.png");
	up-icon: 					Embed(source="../images/icon_expand_up.png");
	over-icon: 					Embed(source="../images/icon_expand_over.png");
	selected-over-icon: 		Embed(source="../images/icon_expand_over.png");
	down-icon: 					Embed(source="../images/icon_expand_down.png");
	selected-down-icon: 		Embed(source="../images/icon_expand_down.png");
	disabled-skin: 				Embed(source="../images/clear.png");
	selected-up-skin: 			Embed(source="../images/clear.png");
	selected-over-skin: 		Embed(source="../images/clear.png");
	selected-down-skin: 		Embed(source="../images/clear.png");
    down-skin: 					Embed(source="../images/clear.png");
    over-skin: 					Embed(source="../images/clear.png");
    up-skin: 					Embed(source="../images/clear.png");	
}

.minimizeButton
{
	icon: 						Embed(source="../images/icon_minimize_up.png");
	up-icon: 					Embed(source="../images/icon_minimize_up.png");
	over-icon: 					Embed(source="../images/icon_minimize_over.png");
	selected-over-icon: 		Embed(source="../images/icon_minimize_over.png");
	down-icon: 					Embed(source="../images/icon_minimize_down.png");
	selected-down-icon: 		Embed(source="../images/icon_minimize_down.png");
	disabled-skin: 				Embed(source="../images/clear.png");
	selected-up-skin: 			Embed(source="../images/clear.png");
	selected-over-skin: 		Embed(source="../images/clear.png");
	selected-down-skin: 		Embed(source="../images/clear.png");
    down-skin: 					Embed(source="../images/clear.png");
    over-skin: 					Embed(source="../images/clear.png");
    up-skin: 					Embed(source="../images/clear.png");
}

.mypanelTitle
{
	font-weight:				bold;
    color: 						#ffffff;
}

.mypanelInactiveTitle
{
	font-weight:				bold;
    color: 						#bbbbbb;
}

.leftBox
{
	border-style:				solid;
	border-sides:				right;
	border-color:				#666666;
	background-color:			#000000;
	background-alpha:			0.6;
	
	padding-top:				20;
	padding-bottom:				20;
	padding-left:				20;
	padding-right:				20;
}

mx|LegendItem {
    fontSize: 12;
}

chartClasses|DataTip {
    fontSize: 12;
	background-color: #FFFFFF;
	color: #000000;
} 

calendarDisplayClasses|CalendarEventRenderer {
	font-family: "Arial"
}

mx|DateChooser{
	background-color: #FFFFFF;
}
