package myreport.design
{
	import hlib.ComponentBase;
 
	internal final class DivideLine extends ComponentBase
	{
 		public var OldValue:Number;
		public function DivideLine()
		{
			super();
		}
		protected override function OnResize():void
		{
			graphics.clear();
			graphics.beginFill(0x696969, 1);
			graphics.drawRect(0, 0, width, height);
			graphics.endFill();
		}
	}
}