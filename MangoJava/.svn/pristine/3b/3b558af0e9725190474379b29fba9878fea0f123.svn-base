package smartx.system.common.interfaces;

/**
 * Public Interface for all Data Objects!
 */
public interface DObjectInterface {
    /**get the ID attribute value of this object.
     */
    public long getId();

    /**get the persistent type value of this object.
     */
    public byte getPersistType();

    /**set the persistent type value of this object.
     */
    public void setPersistType(byte persistType);

    /**get the oid attribute value of this object.
     */
    public int getOid();

    /**set the oid attribute value of this object.
     */
    public void setOid(int oid);
}
