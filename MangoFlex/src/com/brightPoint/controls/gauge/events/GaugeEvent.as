package com.brightPoint.controls.gauge.events
{
	import flash.events.Event;
	
	public class GaugeEvent extends Event
	{
		public var value:Number;
		public var newValue:Number;
		public var x:Number;
		public var y:Number;
		public function GaugeEvent(value:Number,newValue:Number,
								   x:Number,y:Number,type:String="gaugeClick", bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
	}
}