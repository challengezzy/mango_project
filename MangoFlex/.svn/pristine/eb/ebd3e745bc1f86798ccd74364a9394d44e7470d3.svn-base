package smartx.flex.components.core.mtchart.vo
{
	/**
	 * sky zhangzz
	 **/
	public class MTChart
	{
		//行模式
		public static const ROW_MODE:String = "rowMode";
		//列模式
		public static const COLUMN_MODE:String = "columnMode";
		
		public var datasource:String; //数据源
		public var viewName:String; //视图名称
		public var chartName:String;//图表名称
		public var backupChartName:String;//备用图表名称
		public var dataFileds:Array = [];//数据字段名称
		public var nameFileds:Array = [];//图表示例字段名称
		public var labelFileds:Array = [];//图表标签名称
		public var linkFileds:Array = [];//图标超链接字段名称
		public var filter:String;//过滤条件
		public var backupFilter:String;//备用过滤条件
		public var colors:Array = [];//颜色
		public var isShowLabel:Boolean;//是否显示标签
		public var isShow3D:Boolean;//是否显示3D图表
		public var chartType:String;//图表类型
		
		public var horizontalField:String;
		public var verticalField:String;
		
		public var horizontalTitle:String;
		public var verticalTitle:String;
		public var refreshInterval:Number;
		
		//混合图专用属性 start
		public var xAxiaName:String;//x轴名称
		public var pyAxiaName:String;//Y轴名称(主)
		public var syAxiaName:String;//Y轴名称(次)
		public var seriesNames:Array = [];//系列名称
		public var types:Array = [];//图系列类型
		public var lineMode:String;//折线图模式
		public var dataMode:String;//数据模式
		public var subSeriesNames:Array = [];//使用辅助y轴的系列
		public var xShowFields:Array = [];//X轴显示字段
		//end
		
		public var orderby:String;
		public var groupby:String;
		public var isHorizontalLegend:Boolean;//是否水平显示图表示例
		
		public var yAxisMinValue:String;//Y轴最小值
		public var yAxisMaxValue:String;//Y轴最大值
		
		public function MTChart()
		{
		}
	}
}